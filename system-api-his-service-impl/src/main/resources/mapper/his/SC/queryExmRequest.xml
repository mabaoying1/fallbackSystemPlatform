<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- 映射文件，映射到对应的SQL接口 -->

<mapper namespace="com.soft.api.mapper.his.SC.QueryExmRequestMapper">

    <select id="queryExmRequest" parameterType="map" resultType="map">
        select SQDH,BRXM,JCLX,JCXM,JCZT  from emr_jcsq t where t.sqdh = #{RequestId}
    </select>

</mapper>